<h1 class="pb-4">Explore Latest</h1>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
  <% if(thoughts != '') { %>
    <% thoughts.forEach(function(thoughts, index){ %>
    <a href="/thoughts/<%= thoughts._id %>" class="col text-center category__link">
      <div class="pt-1"><%= thoughts.name %></div>
    </a>
    <% }) %>
  <% } else { %>
    <p>No items found.</p>
  <% } %>
</div>